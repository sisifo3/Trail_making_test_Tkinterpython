	def com_test_pepe(self):
		self.text1 = tk.Button(self,text ="1",bg = "pale green",command =lambda *args:self.coun_num1(1))
		self.text1.place(x=200,y=190)
		self.text2 = tk.Button(self,text ="2",bg = "bisque", command= lambda *args:self.coun_num2(2))
		self.text2.place(x=400,y=450)
		self.text3 = tk.Button(self,text ="3",bg = "pale green",command= lambda *args:self.coun_num2(3))
		self.text3.place(x=350,y=330)
		self.text4 = tk.Button(self,text ="4",bg = "orange2",command= lambda *args:self.coun_num2(4))
		self.text4.place(x=550,y=280)
		self.text5 = tk.Button(self,text ="5",bg = "violetRed1",command= lambda *args:self.coun_num2(5))
		self.text5.place(x=510,y=150)
		self.text6 = tk.Button(self,text ="6",bg = "orange2",command= lambda *args:self.coun_num2(6))
		self.text6.place(x=400,y=40)
		self.text7 = tk.Button(self,text ="7",bg = "cyan",command= lambda *args:self.coun_num2(7))
		self.text7.place(x=210,y=60)
		self.text8 = tk.Button(self,text ="8",bg = "tomato2",command= lambda *args:self.coun_num2(8))
		self.text8.place(x=160,y=238)
		self.text9 = tk.Button(self,text ="9",bg = "plum2",command= self.del_all_pepe)
		self.text9.place(x=150,y=398)
	
	def com_test_jose(self):
		self.text1 = tk.Button(self,text ="1",bg = "pale green",command =lambda *args:self.coun_num1(1))
		self.text1.place(x=200,y=190)
		self.text2 = tk.Button(self,text ="2",bg = "bisque", command= lambda *args:self.coun_num2(2))
