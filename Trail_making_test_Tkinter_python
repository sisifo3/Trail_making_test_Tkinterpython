	#Creamos un lugar par poder imprimir los botones impresos.
	def coun_num1(self,numero):
		print(numero)
		timestart=time.gmtime()
		print(timestart)	
			
	def coun_num2(self,numero):
		print(numero)
		

root = Tk()
app = Window(root)
root.wm_title("Trail Making Test")
root.geometry("680x800")
app.mainloop()		
