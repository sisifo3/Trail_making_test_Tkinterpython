	def finalizar_prueba_pepe(self):
		label = Label(self, text = "Gacias.",bg = "snow")
		label.place(x=230,y=10)
		label6 = Label(self, text ="Seleccione la operación que desea realizar. finalizar o repetir test  ", bg = "snow",anchor=W)
		label6.place(x=30,y=200)
		bottonno = tk.Button(self,text ="Repetir",bg = "light blue",command =root.destroy)
		bottonno.place(x=290,y=250)
		bottonsi = tk.Button(self,text ="Finalizar",bg = "light blue",command =root.destroy)
		bottonsi.place(x=170,y=250)
	
