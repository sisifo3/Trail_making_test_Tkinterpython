	def emerg_wind_first_instr_pepe(self):
		Window_fir_ins = tk.Toplevel(app)  
		Window_fir_ins.geometry("500x500")
		Window_fir_ins.configure(background='snow')		
		label = Label(Window_fir_ins, text = "Bienvenido",bg = "snow")
		label.place(x=230,y=10)
		label1 = Label(Window_fir_ins, text ="La siguiente prueba consiste en dos Test", bg = "snow",anchor=W)
		label1.place(x=30,y=30)
		label2 = Label(Window_fir_ins, text ="Trail Making Test", bg = "snow",anchor=W)
		label2.place(x=30,y=50)
		label3 = Label(Window_fir_ins, text ="Test of Stroop ", bg = "snow",anchor=W)
		label3.place(x=30,y=70)
		label5 = Label(Window_fir_ins, text ="En la primera parte el participante deberá", bg = "snow",anchor=W)
		label5.place(x=30,y=90)
		label6 = Label(Window_fir_ins, text ="seleccionar de manera ascendente ", bg = "snow",anchor=W)
		label6.place(x=30,y=110)
		label5 = Label(Window_fir_ins, text ="1--->2---->3---->4---->5---->6....9", bg = "snow",anchor=W)
		label5.place(x=30,y=130)
		bottonsi = tk.Button(Window_fir_ins,text ="Comenzar",bg = "light blue",command =Window_fir_ins.destroy)
		bottonsi.place(x=170,y=250)
	
	
	
