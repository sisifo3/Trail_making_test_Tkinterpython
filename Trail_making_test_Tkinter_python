	def last_window(self):
		Window_last = tk.Toplevel(app)  
		Window_last.geometry("500x500")
		Window_last.configure(background='snow')		
		label = Label(Window_last, text = "Resultados",bg = "snow")
		label.place(x=230,y=10)
