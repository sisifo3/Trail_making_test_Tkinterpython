from tkinter import *
import tkinter as tk
from PIL import Image, ImageTk 
import time


global edad
#edad = 0
class Window(Frame):
	 	
	def __init__(self, master=None):
		Frame.__init__(self, master)
		self.master = master
		self.pack(fill=BOTH, expand=1)
		self.configure(background='snow')
		#global edad
		#self.text0 = tk.Button(self,text ="",bg = "pale green",command =self.com_test)
		#self.text0.place(x=220,y=160)	
		self.label0 = Label(self, text = "Bienvenido prueba de atención",bg = "snow")
		self.label0.place(x=220,y=10)
		self.label1 = Label(self, text ="La siguiente prueba consiste en dos Test", bg = "snow",anchor=W)
		self.label1.place(x=200,y=30)
		self.label2 = Label(self, text ="Trail Making Test", bg = "snow",anchor=W)
		self.label2.place(x=250,y=50)
		self.label3 = Label(self, text ="Test of Stroop ", bg = "snow",anchor=W)
		self.label3.place(x=250,y=70)
		self.label5 = Label(self, text ="En la primera parte el participante deberá", bg = "snow",anchor=W)
		self.label5.place(x=190,y=90)
		self.label6 = Label(self, text ="seleccionar de manera ascendente ", bg = "snow",anchor=W)
		self.label6.place(x=200,y=110)
		self.label7 = Label(self, text ="1--->2---->3---->4---->5---->6....9", bg = "snow",anchor=W)
		self.label7.place(x=200,y=130)
		self.label8 = Label(self, text ="En cada sección de la prueba se tomará el tiempo y el numero de aciertos.", bg = "snow",anchor=W)
		self.label8.place(x=100,y=150)
		
		self.label10 = Label(self, text ="Por favor seleccione la edad del participante.", bg = "snow",anchor=W)
		self.label10.place(x=200,y=150)
		
		#we inicialiate v like int 
		v = IntVar()
		v.set(1)
		self.radio_a=Radiobutton(self, text="11-13", variable=v, value=1,bg ="snow")#.pack(anchor=SE)
		self.radio_a.place(x=300,y=250)
		self.radio_b=Radiobutton(self, text="8-10", variable=v, value=2,bg = "snow")
		self.radio_b.place(x=300,y=290)
		

	
		
		self.label9 = Label(self, text ="Cuando el participante este listo, seleccione la opción comenzar.", bg = "snow",anchor=W)
		self.label9.place(x=150,y=450)
		self.bottonsi = tk.Button(self,text ="Comenzar",bg = "light blue",command = lambda *args:self.edad_print(v))#del_all_zero
		self.bottonsi.place(x=290,y=550)
		
	
			
	def edad_print(self,v):
		#print(v.get())
		value_v = v.get()
		if value_v ==1:
			#pepe = 11 - 13
			print("pepe")
			Window.del_all_zero_pepe(self)
		if value_v ==2:
			#jose = 8 - 10
			print('jose')	
			Window.del_all_zero_jose(self)
		
		
	def del_all_zero_pepe(self):
		self.label0.destroy() #destrui un botton necesito crear otro
		self.label1.destroy()
		self.label2.destroy()
		self.label3.destroy()
		#self.label4.destroy()
		self.label5.destroy()
		self.label6.destroy()
		self.label7.destroy()
		self.label8.destroy()
		self.label9.destroy()
		self.label10.destroy()
		self.bottonsi.destroy()
		self.radio_a.destroy()
		self.radio_b.destroy()
		#self.botton10.destroy()
		#self.botton11.destroy() 
		#Window.colrs_secd(self)	
		Window.com_test_pepe(self)
	
	
